<h1>Invites#index</h1>


<% @invites.each do |invite| %>
	<div>	
		Latitude: <%= invite.latitude %></br>
	  Longitude: <%= invite.longitude %></br>
	  User ID: <%= invite.user_id %></br>
	  Recipient: <%= invite.recipient %></br>
	  Message: <%= invite.message %></br>
	  <%= link_to "Delete invite", invite_path(invite.id), method: :delete, data: { confirm: "Are you sure?"} %>
	</div>
	</br>
<% end %>